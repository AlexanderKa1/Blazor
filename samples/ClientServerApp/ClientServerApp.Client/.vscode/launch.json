{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Blazor Program",
            "type": "blazor",
            "request": "launch",
            "program": "${workspaceRoot}/Program.cs"
        },
        {
            "name": "Attach to Blazor Program",
            "type": "blazor",
            "request": "attach",
            "address": "ws://localhost:5000/__debugger",
            "program": "${workspaceRoot}/Program.cs"
        }
    ]
}